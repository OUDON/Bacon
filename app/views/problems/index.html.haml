= render 'contests/contest_header', { contest: @contest }

= render 'contests/tabs', { active_tab: :problems, contest_id: params[:contest_id] }
.problems
  %table{ class: 'table table-striped table-bordered' }
    %thead
      %tr
        %th.center{ width: '5%' }
        %th.center{ width: '95%' } 問題名
    %tbody
      - @contest.problems.each.with_index do |problem, i|
        %tr
          %td.center= link_to ('A'.ord + i).chr, problem.url, target: :_blank
          %td= link_to problem.title, problem.url, target: :_blank
